# .env.example for RAG Gemini Chatbot

# Required: Your Google Gemini API key
GEMINI_API_KEY=your-gemini-api-key-here

# Required: Path to store ChromaDB data (can be a directory or file path)
CHROMA_PATH=chroma/chroma.sqlite3

# Required: Name for your ChromaDB collection
CHROMA_COLLECTION=rag_collection


# Optional: Gemini model for answer generation (default: gemini-pro)
GEMINI_GENERATION_MODEL=gemini-pro